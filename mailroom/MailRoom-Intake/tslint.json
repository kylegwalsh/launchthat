{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules"
  ],
  "rules": {
    "no-shadowed-variable": false,
    "member-access": false,
    "quotemark": [true, "single", "jsx-single"],
    "ter-indent": [true, 2],
    "no-multi-spaces": 2,
    "prefer-const": true,
    "no-duplicate-imports": true,
    "triple-equals": true,
    "curly": false,
    "no-console": false,
    "semicolon": [true, "always"],
    "variable-name": {
      "options": ["ban-keywords", "check-format", "allow-pascal-case", "allow-snake-case"],
      "severity": "warn"
    },
    "whitespace": [true, "check-branch", "check-operator", "check-typecast", "check-decl", "check-module", "check-type", "check-preblock", "check-type-operator", "check-separator", "check-rest-spread", "check-typecast"],
    "space-before-function-paren": false,
    "ter-prefer-arrow-callback": true,
    "no-unused-expression": true,
    "ordered-imports": false,
    "object-curly-spacing": [true, "always"],
    "space-in-parens": [true, "never"],
    "no-empty": [true, "allow-empty-catch"],
    "no-trailing-whitespace": false,
    "no-var-keyword": true,
    "eofline": false,
    "no-any": false,
    "object-literal-sort-keys": false,
    "class-name": false,
    "max-line-length": [true, 140],
    "one-line": false,
    "one-variable-per-declaration": false,
    "prefer-for-of": false,
    "array-type": false,
    "trailing-comma": false,
    "no-implicit-dependencies": [ true, "dev" ],
    "forin": false,
    "max-classes-per-file": false
  },
  "env": {
    "jest": true
  }
}